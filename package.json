{
	"name": "mqpb",
	"version": "1.0.0",
	"description": "My Moleculer-based microservices project",
	"scripts": {
		"build": "tsc --build tsconfig.json",
		"dev": "cross-env NODE_ENV=development dotenv-flow -p ./env -- ts-node -r tsconfig-paths/register ./node_modules/moleculer/bin/moleculer-runner.js --hot --repl --config moleculer.config.ts",
		"start": "ts-node -r tsconfig-paths/register ./node_modules/moleculer/bin/moleculer-runner.js --config moleculer.config.ts",
		"cli": "moleculer connect ",
		"ci": "jest --watchAll",
		"test": "jest --coverage",
		"majestic": "majestic",
		"lint": "eslint --ext .js,.ts .",
		"dc:up": "docker-compose up --build -d",
		"dc:logs": "docker-compose logs -f",
		"dc:down": "docker-compose down"
	},
	"keywords": [
		"microservices",
		"moleculer"
	],
	"repository": {
		"type": "git",
		"url": "git+"
	},
	"homepage": "#readme",
	"author": "",
	"devDependencies": {
		"@types/bcrypt": "^5.0.0",
		"@types/bcryptjs": "^2.4.2",
		"@types/cookie-parser": "^1.4.3",
		"@types/dotenv-flow": "^3.2.0",
		"@types/jest": "^29.4.0",
		"@types/js-yaml": "^4.0.5",
		"@types/jsonwebtoken": "^9.0.1",
		"@types/lodash": "^4.14.191",
		"@types/mkdirp": "^1.0.2",
		"@types/node": "^18.15.0",
		"@types/randomstring": "^1.1.8",
		"@types/rimraf": "^3.0.2",
		"@types/supertest": "^2.0.12",
		"@types/swagger-jsdoc": "^6.0.1",
		"@types/swagger-stats": "^0.95.8",
		"@types/swagger-ui-dist": "^3.30.1",
		"@typescript-eslint/eslint-plugin": "^5.54.1",
		"@typescript-eslint/parser": "^5.54.1",
		"bcrypt": "^5.1.0",
		"chokidar": "^3.5.3",
		"eslint": "^8.35.0",
		"eslint-config-prettier": "^8.7.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-prefer-arrow": "^1.2.3",
		"eslint-plugin-prettier": "^4.2.1",
		"jest": "^29.5.0",
		"jest-chain": "^1.1.6",
		"jest-circus": "^29.5.0",
		"jest-cli": "^29.5.0",
		"jest-extended": "^3.2.4",
		"jest-junit": "^15.0.0",
		"majestic": "^1.8.1",
		"moleculer-repl": "^0.7.3",
		"open": "^8.4.2",
		"prettier": "^2.8.4",
		"supertest": "^6.3.3",
		"ts-jest": "^29.0.5",
		"ts-node": "^10.9.1",
		"tsconfig-paths": "^4.1.2"
	},
	"dependencies": {
		"@ourparentcenter/moleculer-decorators-extended": "^2.0.0",
		"bcryptjs": "^2.4.3",
		"cookie-parser": "^1.4.6",
		"core-js": "^3.29.0",
		"cross-env": "^7.0.3",
		"csv-parse": "^5.3.6",
		"dotenv": "^16.0.3",
		"dotenv-flow": "^3.2.0",
		"dotenv-flow-cli": "^1.0.0",
		"helmet": "^6.0.1",
		"jose": "^4.13.1",
		"js-yaml": "^4.1.0",
		"json2typescript": "^1.5.1",
		"jsonwebtoken": "^9.0.0",
		"lodash": "^4.17.21",
		"log4js": "^6.9.1",
		"moleculer": "^0.14.28",
		"moleculer-db": "^0.8.23",
		"moleculer-db-adapter-mongo": "^0.4.17",
		"moleculer-db-adapter-mongoose": "^0.9.2",
		"moleculer-web": "^0.10.5",
		"mongodb": "^5.1.0",
		"mongoose": "^7.0.1",
		"openapi-types": "^12.1.0",
		"prom-client": "^14.2.0",
		"randomstring": "^1.2.3",
		"reflect-metadata": "^0.1.13",
		"replace-in-file": "^6.3.5",
		"swagger-editor-dist": "^4.8.0",
		"swagger-jsdoc": "^6.2.8",
		"swagger-stats": "^0.99.5",
		"swagger-ui-dist": "^4.18.0",
		"ts-results": "^3.3.0",
		"typescript": "^4.9.5"
	},
	"engines": {
		"node": ">= 16.x.x"
	},
	"jest": {
		"coverageDirectory": "<rootDir>/coverage",
		"testEnvironment": "node",
		"collectCoverage": false,
		"coveragePathIgnorePatterns": [
			"<rootDir>/moleculer.config.ts",
			"<rootDir>/middlewares/OpenInBrowser.ts"
		],
		"reporters": [
			"default",
			"jest-junit"
		],
		"setupFiles": [
			"./jest.setup.ts"
		],
		"setupFilesAfterEnv": [
			"jest-chain",
			"jest-extended/all"
		],
		"moduleNameMapper": {
			"^@MicroServices/(.*)$": "<rootDir>/services/$1",
			"^@MicroServices(.*)$": "<rootDir>/services/$1",
			"^@Entities/(.*)$": "<rootDir>/entities/$1",
			"^@Entities(.*)$": "<rootDir>/entities/$1",
			"^@Factories/(.*)$": "<rootDir>/factories/$1",
			"^@Factories(.*)$": "<rootDir>/factories/$1",
			"^@Middlewares/(.*)$": "<rootDir>/middlewares/$1",
			"^@Middlewares(.*)$": "<rootDir>/middlewares/$1",
			"^@Mixins/(.*)$": "<rootDir>/mixins/$1",
			"^@Mixins(.*)$": "<rootDir>/mixins/$1",
			"^@ServiceHelpers/(.*)$": "<rootDir>/helpers/$1",
			"^@ServiceHelpers(.*)$": "<rootDir>/helpers/$1"
		},
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js"
		],
		"transform": {
			"^.+\\.(ts|tsx)$": [
				"ts-jest",
				{
					"tsconfig": "tsconfig.json",
					"diagnostics": true
				}
			]
		},
		"testMatch": [
			"**/*.spec.(ts|js)"
		]
	}
}
